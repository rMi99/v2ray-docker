{
    "inbounds": [
      {
        "port": 1080,
        "protocol": "socks",
        "settings": {
          "auth": "noauth",
          "udp": true
        }
      }
    ],
    "outbounds": [
      {
        "protocol": "freedom",
        "settings": {},
        "tag": "direct"
      }
    ],
    "routing": {
      "rules": [
        {
          "type": "field",
          "domain": ["*"],  
          "outboundTag": "redirect"
        }
      ]
    },
    "outboundDetour": [
      {
        "protocol": "freedom",
        "tag": "redirect",
        "settings": {
          "domainStrategy": "useIP",
          "redirect": {
            "host": "m.youtube.com"
          }
        }
      }
    ]
  }
  